page {
  background: rgba(247, 247, 247, 1);
}

/* 轮播图图片尺寸 */

.home-swiper {
  width: 100%;
  height: 419rpx;
  position: relative;
}

.home-swiper image {
  z-index: 9999;
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}

.two-swiper {
  width: 340rpx;
  height: 225rpx;
  margin-left: 25rpx;
  margin-top: 20rpx;
}

.content {
  width: 100%;
  height: 100%;
}

.contenter {
  width: 100%;
  padding-top: 314rpx;
  opacity: 0;
}

._contenter {
  width: 702rpx;
  height: 570rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 12px 0px 21px rgba(247, 247, 247, 0.6);
  margin: 24rpx auto;
  border-radius: 10rpx;
}

.contentImg {
  width: 100%;
  height: 350rpx;
  position: relative;
}

.contentText {
  font-size: 33rpx;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
  margin-left: 25rpx;
  margin-top: 8rpx;
}

.areaContent {
  font-size: 28rpx;
  margin-top: 15rpx;
  margin-left: 25rpx;
  color: rgba(102, 102, 102, 1);
}

.publish {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  bottom: 110rpx;
  right: 50rpx;
}

.publish image {
  width: 100%;
  height: 100%;
}

.two-enter {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}

.enterimg {
  width: 340rpx;
  height: 225rpx;
  border-radius: 10rpx;
}

.evaluate {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  /* padding-left: 25rpx; */
  padding: 0 25rpx;
  /* justify-content: space-between; */
  justify-content: flex-end;
}

.const {
  font-size: 32rpx;
  margin-left: 10rpx;
}

.labelsline {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-left: 10rpx;
  align-items: center;
}

.labels text {
  font-size: 30rpx;
  color: #000;
}

.titile-price {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  justify-content: space-between;
  padding: 0 25rpx;
}

.price {
  height: auto;
  color: #fa6d22;
  font-size: 30rpx;
  padding: 5rpx 10rpx;
}

.original {
  font-size: 25rpx;
  color: #999;
  text-decoration: line-through;
  margin-left: 15rpx;
  margin-right: 15rpx;
}

.disance {
  font-size: 32rpx;
}

.emp-line {
  width: 100%;
  text-align: center;
}
