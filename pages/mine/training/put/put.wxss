page {
  background: rgba(247, 247, 247, 1);
}

.put-container {
  width: 702rpx;
  height: auro;
  padding-top: 20rpx;
  padding-bottom: 35rpx;
  margin: 20rpx auto;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  box-shadow: 0px 0px 0px rgba(102, 102, 102, 0.2);
}

.put-con0 {
  margin-left: 25rpx;
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
}

.put-c-con0 {
  width: 100%;
  font-size: 28rpx;
  color: #999;
  margin-top: 60rpx;
  margin-left: 25rpx;
}

.put-c-con01 {
  width: 100%;
  font-size: 28rpx;
  color: #999;
  margin-top: 30rpx;
  margin-left: 25rpx;
}

.put-input {
  border-bottom: 1rpx solid #efefef;
  margin-top: 35rpx;
  padding-left: 10rpx;
  width: 100%;
  padding-bottom: 10rpx;
}

input {
  font-size: 35rpx;
  color: rgba(51, 51, 51, 1);
  width: 68%;
}

.inputText {
  font-size: 35rpx;
  color: rgba(255, 68, 39, 1);
}

button {
  background: #fa6d22;
  width: 480rpx;
  height: 80rpx;
  background: linear-gradient(80deg, rgba(250, 109, 34, 1), rgba(247, 96, 60, 1));
  color: #fff;
  line-height: 80rpx;
  border-radius: 8rpx;
  margin-top: 80rpx;
}

.inputContent {
  margin: 25rpx auto;
  /* padding-bottom: 110rpx; *//* text-align: center; */
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  /* justify-content: center; */
  height: auto;
  color: #999;
  width: 90%;
}

.boxinput {
  width: 100%;
  justify-content: space-between;
  margin: 20rpx auto;
}

/* .boxinput input{
  text-align: right;
} */

.boxinput text {
  padding-left: 25rpx;
}

/*客服  */

.card-img {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 30rpx;
  bottom: 135rpx;
}

button::after {
  background: none;
}

.buy {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 30rpx;
  bottom: 135rpx;
  opacity: 0;
}

.pwd {
  width: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: flex-end;
  color: #999;
  padding-right: 20rpx;
  text-decoration: underline;
}

.alert {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 666;
}

.alert-box {
  background: rgba(0, 0, 0, 0.8);
  z-index: 999;
  border-radius: 8rpx;
  width: 70%;
  color: #fff;
  padding: 0 25rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}
