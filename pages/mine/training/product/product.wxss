page {
  background: rgba(239, 239, 239, 1);
}

.produc-container {
  width: 702rpx;
  height: 108rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  margin: 20rpx auto;
  box-shadow: 12rpx 0px 21rpx rgba(153, 153, 153, 0.2);
}

.pro-right {
  width: 24rpx;
  height: 24rpx;
  margin-left: 25rpx;
}

.pro-content {
  width: 702rpx;
  height: auto;
  padding-top: 20rpx;
  padding-bottom: 36rpx;
  margin: 20rpx auto;
  border-radius: 20rpx;
  background: rgba(255, 255, 255, 1);
  justify-content: center;
}

.pro-box {
  width: 95%;
  border-bottom: 1rpx solid #efefef;
  margin-top: 20rpx;
  margin: 10rpx auto;
}

.pro-image {
  width: 220rpx;
  height: 198rpx;
}

.pro-box-right {
  margin-left: 25rpx;
  width: 418rpx;
}

.box-right-top {
  width: 100%;
  justify-content: space-between;
  height: 150rpx;
}

.select-right {
  font-size: 30rpx;
  color: rgba(253, 130, 48, 1);
}

._select-right {
  font-size: 30rpx;
  color: #999;
  padding-right: 15rpx;
}

.box-price {
  font-size: 28rpx;
  color: rgba(253, 130, 48, 1);
}

.box-price text {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  text-decoration: line-through;
  margin-left: 25rpx;
}

.box-time {
  font-size: 28rpx;
  margin-top: 25rpx;
  margin-bottom: 25rpx;
  color: rgba(51, 51, 51, 1);
  padding-left: 12rpx;
}

.box-name {
  width: 300rpx;
  height: 48rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
