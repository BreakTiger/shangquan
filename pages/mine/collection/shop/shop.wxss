page {
  background: #fff;
}

.shop {
  width: 100%;
  align-items: center;
  height: auto;
}

.shoptext {
  margin-top: 25rpx;
  font-size: 46rpx;
  color: rgba(51, 51, 51, 1);
  margin-left: 35rpx;
}

.shopContent {
  margin-top: 25rpx;
  margin-left: 35rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  color: rgba(153, 153, 153, 1);
  padding-bottom: 25rpx;
}

.inputs {
  width: 100%;
  justify-content: center;
  align-items: center;
}

.input-box {
  justify-content: space-between;
  width: 90%;
  height: 89rpx;
  border-bottom: 1rpx solid #efefef;
}

.input-img {
  width: 90%;
  height: 89rpx;
}

input {
  text-align: right;
  font-size: 28rpx;
  width: 260rpx;
}

.right-right {
  width: 11rpx;
  height: 19rpx;
  position: absolute;
  right: 53rpx;
}

.up-pic {
  /* padding:20rpx 24rpx; */
  width: 100%;
  justify-content: center;
}

.pic-box {
  margin-top: 20rpx;
  flex-flow: wrap;
  width: 95%;
  /* justify-content: center; */
}

.ap-box {
  position: relative;
}

.add-pic {
  width: 270rpx;
  height: 270rpx;
  margin-right: 20rpx;
  position: relative;
  margin: 20rpx 30rpx 20rpx 30rpx;
  margin-top: 15rpx;
}

/* 删除图片 */

.img-de {
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  position: absolute;
  right: -50rpx;
  top: -4rpx;
}

button {
  margin: 50rpx auto;
  width: 480rpx;
  height: 80rpx;
  background: linear-gradient(0deg, rgba(250, 109, 34, 1), rgba(247, 96, 60, 1));
  border-radius: 8rpx;
  color: #fff;
  line-height: 80rpx;
}

.section {
  margin-right: 50rpx;
}

/* 日期选择 */

.picker_group {
  height: 85rpx;
  line-height: 85rpx;
  justify-content: center;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #888;
  width: 307rpx;
}

.picker_group picker {
  /* background-color: yellow; */
  color: #000;
  height: 55rpx;
  line-height: 55rpx;
  /* margin: 0 2%; *//* padding: 0 2%; */
}

.center {
  width: 120rpx;
  height: 120rpx;
}

.header {
  width: 100%;
  margin: 50rpx auto;
}

.header-to {
  margin: 30rpx auto;
}

.back {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
}

.area_ {
  width: 22rpx;
  height: 28rpx;
}

.argee {
  width: 90%;
  margin: 15rpx auto;
}

/* 遮罩 */

.select-alert {
  width: 100%;
  position: fixed;
  background: rgba(0, 0, 0, 0.6);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
}

.mask {
  width: 480rpx;
  height: 600rpx;
  border-radius: 10rpx;
  margin: 0 auto;
}

.alert {
  width: 90%;
  margin: 0 auto;
  z-index: 999;
  background: #fff;
}

.zheZhao-top {
  width: 100%;
}

.bg {
  position: fixed;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1;
  -moz-opacity: 0.3;
  opacity: 0.30;
  filter: alpha(opacity=30);
}

.zheZhao-top {
  width: 620rpx;
  height: 680rpx;
  /* background-color:#f7f7f7;  */
  display: flex;
  flex-direction: column;
  align-content: center;
  position: fixed;
  left: 68rpx;
  top: 140rpx;
  border-radius: 20rpx;
  overflow: hidden;
}

/* 排行榜 */

.houlai {
  width: 668rpx;
  height: 868rpx;
  z-index: 999;
  position: fixed;
  left: 40rpx;
  top: 20rpx;
  /* overflow: hidden */
}

/*  */

.box-zhe {
  padding: 0 40rpx;
  height: 119rpx;
  border-bottom: 1rpx #efefef solid;
  line-height: 119rpx;
}

.name-zhe {
  color: #333;
  font-size: 28rpx;
  margin-bottom: 60rpx;
  position: relative;
  bottom: 20rpx;
}

.weight-zhe {
  color: #666;
  float: right;
}

.number {
  color: #666;
  font-size: 28rpx;
  position: relative;
  left: 10rpx;
  bottom: 20rpx;
  margin-right: 53rpx;
}

.zheImg {
  width: 40rpx;
  height: 48rpx;
  margin-right: 30rpx;
}

.zhao-img {
  width: 60rpx;
  height: 60rpx;
  margin-top: 36rpx;
  margin-right: 20rpx;
}

/*取消按钮*/

.cancel_btn {
  width: 80rpx;
  height: 80rpx;
  margin: 40rpx auto;
  position: fixed;
  z-index: 3;
  bottom: 100rpx;
  left: 330rpx;
}

.cancel_btn image {
  width: 100%;
  height: 100%;
}

/* -------------------------- */

.nav {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}

.default {
  line-height: 60rpx;
  text-align: center;
  color: #000;
  font-size: 28rpx;
  /* background-image: linear-gradient(#f6d97c 0%, #f69545 100%); */
  width: 180rpx;
  height: 60rpx;
  margin-left: 20rpx;
  border-radius: 80rpx;
}

.red {
  line-height: 60rpx;
  margin-left: 20rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background-image: linear-gradient(#f6d97c 0%, #f69545 100%);
  width: 180rpx;
  height: 60rpx;
  border-radius: 80rpx;
}

.show {
  display: block;
}

.hidden {
  display: none;
}
